<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>AutomationFoundation.Runtime</RootNamespace>
    <Version>1.0.0</Version>
    <PackageVersion>$(Version)</PackageVersion>
    <InformationalVersion>$(Version)</InformationalVersion>
    <Authors>Winnster.io</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryType>git</RepositoryType>
    <PackageTags>automation,automationfoundation,runtime</PackageTags>
    <RepositoryUrl>https://github.com/winnster/AutomationFoundation.Runtime.Abstractions</RepositoryUrl>
    <Description>Contains the abstractions for the AutomationFoundation runtime.</Description>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>obj\AutomationFoundation.Runtime.Abstractions.xml</DocumentationFile>
  </PropertyGroup>

</Project>
