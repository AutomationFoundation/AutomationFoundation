<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>AutomationFoundation.Transactions.Abstractions</Product>
    <Version>1.0.0</Version>
    <PackageVersion>$(Version)</PackageVersion>
    <InformationalVersion>$(Version)</InformationalVersion>
    <Authors>Winnster.io</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Contains the abstractions for transactions used within the AutomationFoundation runtime.</Description>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/winnster/AutomationFoundation.Transactions.Abstractions</RepositoryUrl>
    <PackageTags>automation,automationfoundation,transactions</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <DocumentationFile>obj\AutomationFoundation.Transactions.Abstractions.xml</DocumentationFile>
  </PropertyGroup>
 
</Project>
